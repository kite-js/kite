{
    "name": "kite-framework",
    "version": "0.5.5",
    "description": "Next generation, fast, flexible HTTP-JSON-RPC framework",
    "scripts": {
        "setup": "npm install",
        "build": "tsc && cp ./{package.json,README.md} dist/",
        "pub": "pushd ./dist && npm publish --registry=https://registry.npmjs.org && popd",
        "clean": "rm -rf ./dist",
        "typedoc": "typedoc --out ./api_docs --mode modules --tsconfig ./tsconfig.json ./src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kite-js/kite.git"
    },
    "keywords": [
        "Typescript",
        "RPC",
        "framework"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "author": "Arthur Xie",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kite-js/kite/issues"
    },
    "homepage": "https://github.com/kite-js/kite#readme",
    "dependencies": {
        "@types/node": "^8.0.14",
        "reflect-metadata": "^0.1.10"
    },
    "devDependencies": {
        "tslint": "^5.6.0",
        "typedoc": "^0.11.1",
        "typescript": "^2.4.2"
    }
}
